{
    "name": "get-tracks-data",
    "version": "0.4.1",
    "description": "Get video / audio / subtitles tracks from MKV and MP4 files.",
    "main": "dist/index.js",
    "scripts": {
        "prepublish": "npm run build",
        "build": "babel src -d dist && esbuild dist/index.js --bundle --minify --platform=node --outfile=dist/index.js --allow-overwrite"
    },
    "files": [
        "dist"
    ],
    "keywords": [
        "mp4",
        "mkv",
        "tracks",
        "data",
        "moov",
        "ebml"
    ],
    "author": "tymmesyde",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/tymmesyde/get-tracks-data.git"
    },
    "devDependencies": {
        "@babel/cli": "^7.24.5",
        "@babel/core": "^7.24.5",
        "@babel/preset-env": "^7.24.5",
        "core-js": "^3.37.1",
        "esbuild": "^0.21.3",
        "ts-ebml": "^3.0.1"
    }
}
